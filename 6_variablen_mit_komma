local a = 365
local b = 7
local stringa = "Ein Jahr"
local stringb = "Woche"
local stringc = "Wochen"

print(stringa.." hat "..a.." Tage. Eine "..stringb.." hat "..b.." Tage. Ein Jahr hat im gesamten "..a/b.." "..stringc..".")

print(stringa, "hat", a, "Tage. Eine ", stringb, "hat", b, "Tage. Ein Jahr hat im gesamten", a/b, stringc, "." )
